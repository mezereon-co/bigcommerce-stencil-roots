{
  "root": true,
  "env": {
    "node": true,
    "browser": true
  },
  "extends": [
    "plugin:vue/base",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "parser": "@babel/eslint-parser"
  },
  "rules": {
    "nuxt/no-cjs-in-config": "off",
    "nuxt/no-env-in-hooks": "off",
    "vue/no-v-html": "off",
    "no-console": "off"
  }
}
