{
  "name": "@mezereon/bigcommerce-stencil-roots",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mezereon-co/bigcommerce-stencil-roots"
  },
  "version": "1.0.2",
  "description": "UI Components for BigCommerce Roots Theme",
  "author": "Mezereon Inc.",
  "license": "MIT",
  "keywords": [
    "vue",
    "search",
    "bigcommerce",
    "roots",
    "mezereon"
  ],
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "SET NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build --report --target lib --formats umd-min --name ui-components ./src/index.js",
    "lint": "vue-cli-service lint"
  },
  "files": [
    "dist/*",
    "src/*",
    "LICENSE.md",
    "README.md",
    "vue.config.js",
    "babel.config.js",
    "package.json"
  ],
  "dependencies": {
    "vue": "^2.7.16",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^7.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "node-sass": "^8.0.0",
    "sass-loader": "^13.3.3",
    "vue-template-compiler": "^2.7.16"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "main": "./dist/ui-components.umd.min.js"
}
